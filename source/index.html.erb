<%= partial "layouts/banner" %>
<div class="container">
  <div class="row">
    <% data.cities.each do |city| %>
      <div class="col-md-4">
        <div class="card" style="background-image: url('https://kitt.lewagon.com/placeholder/cities/<%= city.name %>');">
          <h2><%= city.name.capitalize %></h2>
          <p><%= city.description %></p>
        </div>
      </div>
    <% end %>
  </div>
</div>

